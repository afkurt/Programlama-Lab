from sympy import Symbol, pprint
import sympy as sym
import matplotlib.pyplot as plt
%matplotlib inline
In [3]:
p = Symbol('p') #p = olasılık ifadesi
n = Symbol('n') #n = örneklem  uzayı ifadesi
x = Symbol('x') #x = örnek olay ifadesi
